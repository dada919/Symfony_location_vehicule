{% extends 'base.html.twig' %}

{% block body %}
    <h2>Connexion</h2>

    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}

    {{ form_start(form) }}
    {{ form_widget(form.username) }}
    {{ form_widget(form.password) }}
    <button type="submit">Se connecter</button>
    {{ form_end(form) }}
{% endblock %}
